    <script type = "text/JavaScript">

      let dias = ["Lunes", "Martes", "Miercoles", "Jueves", "Viernes", "Sabado","Domingo"];
      let meses = ["Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio", "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"];
      
      function diaSemana(){
        
        let date  = new Date(document.getElementById("fecha").value);
        let fechaNum = date.getUTCDate();
        let mes_name = date.getMonth();
        let semana = ((date.getUTCDate()+1)/7);
        let semanaR= Math.ceil(semana);


      document.f1.dia.value = fechaNum
      document.f1.mes.value = meses[mes_name]
      document.f1.semanas.value = semanaR
 
     }




    </script>
